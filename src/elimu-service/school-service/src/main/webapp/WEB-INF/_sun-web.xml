<sun-web-app>
    <!--  ack for running it into EmbeddedGlassfishServer  -->
    <context-root>/elimu-school-service</context-root>
    <resource-ref>
        <res-ref-name>jdbc/elimu</res-ref-name>
        <mapped-name>jdbc/_default</mapped-name>
        <res-type>java.sql.DataSource</res-type>
        <injection-target>
            <injection-target-name>jdbc/elimu</injection-target-name>
        </injection-target>
    </resource-ref>
    <class-loader delegate="true"/>
</sun-web-app>
